generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Passwords {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  associated  String
  password    String
  status      String
  lastUsed    DateTime?
  categoryIds String[]
  userId      String
}

model Categories {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  name     String
}